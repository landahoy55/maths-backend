{
  "name": "maths-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "NODE_ENV=develpment nodemon -w src --exec \"babel src --presets es2015\"",
    "start": "node ./dist/app.js",
    "build": "babel src -s -D -d dist --presets es2015 --ignore 'src/**/*.test.js'",
    "test": "mocha src/**/*.test.js --compilers js:babel-register",
    "lint": "eslint src"
  },
  "engines": {
    "node": "8.7.0"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "author": "Pete Malone",
  "license": "ISC",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "body-parser": "^1.18.2",
    "express": "^4.16.2",
    "express-jwt": "^5.3.0",
    "jsonwebtoken": "^8.1.1",
    "mongodb": "^3.0.2",
    "mongoose": "^5.0.6",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^4.5.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "babel-register": "^6.26.0",
    "eslint": "^4.18.0",
    "expect": "^22.4.0",
    "mocha": "^5.0.1",
    "nodemon": "^1.15.0",
    "supertest": "^3.0.0"
  }
}
